<h1>Create an Account</h1>
<%= @user.errors.full_messages.join(", ") %>
<%= simple_form_for @user do |f| %>
  <div class="content text-left" >

    <%= f.input :first_name %>

    <%= f.input :last_name %>

    <%= f.input :email %>

    <%= f.input :password %>

    <%= f.input :password_confirmation %>
  </div>
  <%= f.button :submit, class: "btn btn-default btn-primary" %>
<% end %>
